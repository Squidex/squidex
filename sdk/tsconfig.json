{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "jsx": "react", 
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ES5"
    },
    "include": ["src/**/*", "tests/**/*"]
}
