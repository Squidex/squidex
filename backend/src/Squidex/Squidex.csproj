    <PackageReference Include="OpenTelemetry.Instrumentation.Http" Version="1.0.0-rc7" />
    <PackageReference Include="RefactoringEssentials" Version="5.6.0" PrivateAssets="all" />
    <PackageReference Include="ReportGenerator" Version="5.1.11" PrivateAssets="all" />
    <PackageReference Include="Squidex.Assets.Azure" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.GoogleCloud" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.FTP" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.ImageMagick" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.ImageSharp" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.Mongo" Version="5.0.0" />
    <PackageReference Include="Squidex.Assets.S3" Version="5.0.0" />
