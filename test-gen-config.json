{
  "enabled": "true",
  "increase-coverage": {
    "project-name": "dotnet-squidex",
    "target-branches": [
      "main",
      "master",
      "develop"
    ],
    "test-env-docker-stage": "test",
    "build-test-env-docker-stage-script-path": "tool/build-test-env-docker-stage.sh",
    "test-files-folders": [
      "backend/tests/Squidex.Domain.Apps.Entities.Tests/Apps/DefaultAppLogStoreTests.cs"
    ],
    "source-test-file-paths": [
      {
        "source-file-path": "backend/src/Squidex.Domain.Apps.Entities/Apps/DefaultAppLogStore.cs",
        "test-file-path": "backend/tests/Squidex.Domain.Apps.Entities.Tests/Apps/DefaultAppLogStoreTests.cs"
      }
    ],
    "test-command": "cd backend && dotnet test --filter \"Category!=Dependencies & Category!=TestContainer\" --configuration Release --collect:\"XPlat Code Coverage\" --settings coverlet.runsettings.xml --results-directory TestResults --logger trx",
    "coverage-type": "cobertura",
    "code-coverage-report-path": "backend/TestResults/*/coverage.cobertura.xml",
    "desired-coverage": 80,
    "max-iterations": 3,
    "max-number-of-test-cases": 8
  }
}
